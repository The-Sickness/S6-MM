		       <&clock 302>, <&clock 303>, <&clock 306>, <&clock 307>;
		clock-names = "usb_pll", "phy_otg", "otg_aclk", "otg_hclk",
			"upsizer_otg", "xiu_d_fsys1", "upsizer_fsys1", "upsizer_ahb_usbhs";
